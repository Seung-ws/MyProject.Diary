<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="eun.myself.myapp.member.dao.IMemberRepository">
	<insert id="signUpMember" parameterType="eun.myself.myapp.member.model.Member">
	<![CDATA[
		insert into 
			memberInfo 
			(uid,gid,userid,username,password,email,profileimage)
		values
			(#{uid},#{gid},#{username},#{username},#{password},#{email},#{profileimage})
	]]>
	</insert>
	
	<select id="memberSignin" resultType="eun.myself.myapp.member.model.Member">
		Select * from a
	</select>
	<update id="memberUpdate" parameterType="eun.myself.myapp.member.model.Member">
	<![CDATA[
		update 
		 memberInfo
	    set  
	    username=#{username}, 
	     password=#{password}, 
	     email=#{email} 
	    where 
	    userid=#{userid}
	]]>
		
	</update>
	<delete id="memberDelete" parameterType="string">
	<![CDATA[
		delete
		 from
		  memberInfo 
	     where
	      userid=#{userid}
	]]>
		
	</delete>
	<select id="selectMember" parameterType="string" resultType="eun.myself.myapp.member.model.Member">
	<![CDATA[
		Select 
			uid,
			gid,
			userid,
			username,
			password,
			email,
			profileimage
		 from
		  memberInfo
	     where userid=#{userid}
   	]]>
	</select>
</mapper>